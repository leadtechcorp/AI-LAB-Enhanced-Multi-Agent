# LangGraph Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Key (if using OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (if using Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LangSmith (for tracing and monitoring)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=AI-LAB-Customer-Service-Dev

# Custom environment variables for your application
APP_NAME=AI-LAB-Customer-Service-Dev
VERSION=1.0.0
